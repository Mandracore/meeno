      "_": true,
      "Backbone": true,
      "require": true,
      "define": true,
      "jasmine": true,
      "isCommonJS": true,
      "exports": true,
      "spyOn": true,
      "it": true,
      "xit": true,
      "expect": true,
      "runs": true,
      "waits": true,
      "waitsFor": true,
      "beforeEach": true,
      "beforeAfter": true,
      "describe": true,
      "xdescribe": true
    },
    "reporter": "checkstyle",
    "reporterOutput": "jshint-output.xml"
  }
